FUNCTION cmpr_2mc (dataa[1..0], datab[1..0])
RETURNS ( aeb);

--synthesis_resources = 
SUBDESIGN decode_ore
( 
	data[1..0]	:	input;
	eq[3..0]	:	output;
) 
VARIABLE 
	cmpr1 : cmpr_2mc;
	cmpr2 : cmpr_2mc;
	cmpr3 : cmpr_2mc;
	cmpr4 : cmpr_2mc;
	const_data0w[1..0]	: WIRE;
	const_data1w[1..0]	: WIRE;
	const_data2w[1..0]	: WIRE;
	const_data3w[1..0]	: WIRE;
	eq_node[3..0]	: WIRE;

BEGIN 
	cmpr1.dataa[] = data[];
	cmpr1.datab[] = const_data0w[];
	cmpr2.dataa[] = data[];
	cmpr2.datab[] = const_data1w[];
	cmpr3.dataa[] = data[];
	cmpr3.datab[] = const_data2w[];
	cmpr4.dataa[] = data[];
	cmpr4.datab[] = const_data3w[];
	const_data0w[] = B"00";
	const_data1w[] = B"01";
	const_data2w[] = B"10";
	const_data3w[] = B"11";
	eq[] = eq_node[];
	eq_node[] = ( cmpr4.aeb, cmpr3.aeb, cmpr2.aeb, cmpr1.aeb);
END;
