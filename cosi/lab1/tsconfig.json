{
  "exclude": [
    ".idea",
    ".vscode"
  ],
  "compileOnSave": true,
  "compilerOptions": {
    "target": "es2015",
    "module": "commonjs",
    "baseUrl": "types",
    "types": [
      "webix",
      "node"
    ],
    "noImplicitAny": false,
    "allowJs": false,
    "noResolve": true,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true
  }
}
